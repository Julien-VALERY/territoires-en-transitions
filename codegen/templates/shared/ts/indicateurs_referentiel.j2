import {IndicateurReferentiel} from "../models/indicateur_referentiel";

export const indicateurs: IndicateurReferentiel[] = [
{% for indicateur in indicateurs recursive %}
    new IndicateurReferentiel({
    id: "{{ indicateur.id }}",
    id_nomenclature: "{{ indicateur.id_nomenclature }}",
    nom: {{ indicateur.nom | tojson }},
    description: {{ indicateur.description | tojson }},
    thematique_id: "{{ indicateur.thematique_id }}",
    unite: {{ indicateur.unite | tojson }}
    }),
{% endfor %}
]
